      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
      "config": {
        "input": {
          "input": "Initializing parallel processing workflow..."
        }
      },
      "dependencies": []
    },
    {
      "node_id": "split_data",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
      "config": {
        "input": {
          "input": "Splitting data into multiple chunks..."
        }
      },
      "dependencies": ["init_workflow"]
    },
    {
      "node_id": "process_chunk_1",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
      "config": {
        "input": {
          "input": "Processing chunk 1..."
        }
      },
      "dependencies": ["split_data"]
    },
    {
      "node_id": "process_chunk_2",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
      "config": {
        "input": {
          "input": "Processing chunk 2..."
        }
      },
      "dependencies": ["split_data"]
    },
    {
      "node_id": "process_chunk_3",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
