        }
      },
      "dependencies": []
    },
    {
      "node_id": "analyze_requirements",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
      "config": {
        "input": {
          "input": "Analyzing business requirements..."
        }
      },
      "dependencies": ["initialize"]
    },
    {
      "node_id": "branch_a_task1",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
      "config": {
        "input": {
          "input": "Branch A: Executing task 1..."
        }
      },
      "dependencies": ["analyze_requirements"]
    },
    {
      "node_id": "branch_a_task2",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
      "config": {
        "input": {
          "input": "Branch A: Executing task 2..."
        }
      },
      "dependencies": ["branch_a_task1"]
    },
    {
      "node_id": "branch_b_task1",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
