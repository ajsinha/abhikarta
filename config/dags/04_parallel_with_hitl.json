        "input": {
          "input": "Branch B: Executing task 1..."
        }
      },
      "dependencies": ["analyze_requirements"]
    },
    {
      "node_id": "branch_b_task2",
      "node_type": "agent",
      "agent_id": "echo_mcp_tool_echo",
